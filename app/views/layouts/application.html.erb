<!DOCTYPE html>
<html>
<head>
  <title>V1</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if user_signed_in? %>
  <%= render 'layouts/registered_user_nav' %>
<% else %>
  <%= render 'layouts/non_user_nav' %>
<% end %>

<div class="container">
	<%= yield %>
</div><!-- end container -->
</body>
</html>
